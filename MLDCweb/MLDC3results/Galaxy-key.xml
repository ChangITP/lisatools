<?xml version="1.0"?>
<!DOCTYPE XSIL SYSTEM "http://www.vallis.org/lisa-xml.dtd">
<?xml-stylesheet type="text/xsl" href="lisa-xml.xsl"?>
<XSIL>
    <XSIL Type="SourceData">
        <XSIL Name="SMBH" Type="PlaneWave">
            <Param Name="SourceType">
                GalacticBinary
            </Param>
            <Param Name="Frequency" Unit="Hertz">
                [Hertz]
            </Param>
            <Param Name="FrequencyDerivative" Unit="Hertz/Second">
                [Hertz/Second, leave empty if undetermined]
            </Param>
            <Param Name="EclipticLatitude" Unit="Radian">
                [-pi/2,pi/2]
            </Param>
            <Param Name="EclipticLongitude" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="Amplitude" Unit="1">
                [strain]
            </Param>
            <Param Name="Inclination" Unit="Radian">
                [0,pi]
            </Param>            
            <Param Name="Polarization" Unit="Radian">
                [0,pi]
            </Param>
            <Param Name="InitialPhase" Unit="Radian">
                [0,2pi]
            </Param>
        </XSIL>
    </XSIL>
</XSIL>
