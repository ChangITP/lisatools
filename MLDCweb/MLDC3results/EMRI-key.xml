<?xml version="1.0"?>
<!DOCTYPE XSIL SYSTEM "http://www.vallis.org/lisa-xml.dtd">
<?xml-stylesheet type="text/xsl" href="lisa-xml.xsl"?>
<XSIL>
    <XSIL Type="SourceData">
        <XSIL Name="EMRI" Type="PlaneWave">
            <Param Name="SourceType">
                ExtremeMassRatioInspiral
            </Param>
            <Param Name="EclipticLatitude" Unit="Radian">
                [-pi/2,pi/2]
            </Param>
            <Param Name="EclipticLongitude" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="MassOfSMBH" Unit="SolarMass">
                [solar masses]
            </Param>
            <Param Name="MassOfCompactObject" Unit="SolarMass">
                [solar masses]
            </Param>
            <Param Name="Spin" Unit="MassSquared">
                [0,1]
            </Param>            
            <Param Name="PolarAngleOfSpin" Unit="Radian">
                [0,pi]
            </Param>
            <Param Name="AzimuthalAngleOfSpin" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="InitialAzimuthalOrbitalFrequency" Unit="Hertz">
                [hertz]
            </Param>
            <Param Name="InitialAzimuthalOrbitalPhase" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="InitialEccentricity" Unit="Unit">
                [0,1]
            </Param>
            <Param Name="InitialTildeGamma" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="InitialAlphaAngle" Unit="Radian">
                [0,2pi]
            </Param>
            <Param Name="LambdaAngle" Unit="Radian">
                [0,pi]
            </Param>
            <Param Name="Distance" Unit="Parsec">
                [parsecs]
            </Param>
        </XSIL>
    </XSIL>
</XSIL>
